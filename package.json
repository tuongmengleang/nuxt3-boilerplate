    "eslint-loader": "^4.0.2",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "nuxt": "3.0.0-rc.6",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
